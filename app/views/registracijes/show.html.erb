<%- model_class = Registracije -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:car_id) %>:</strong></dt>
  <dd><%= @registracije.car_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:car_exp_date) %>:</strong></dt>
  <dd><%= @registracije.car_exp_date %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              registracijes_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_registracije_path(@registracije), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              registracije_path(@registracije),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
