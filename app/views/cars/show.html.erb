<%- model_class = Car -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:user_id) %>:</strong></dt>
  <dd><%= @car.user_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:plate) %>:</strong></dt>
  <dd><%= @car.plate %></dd>
  <dt><strong><%= model_class.human_attribute_name(:registration_date) %>:</strong></dt>
  <dd><%= @car.registration_date %></dd>
  <dt><strong><%= model_class.human_attribute_name(:username) %>:</strong></dt>
  <dd><%= @car.username %></dd>
  <dt><strong><%= model_class.human_attribute_name(:userphone) %>:</strong></dt>
  <dd><%= @car.userphone %></dd>
  <dt><strong><%= model_class.human_attribute_name(:user_name) %>:</strong></dt>
  <dd><%= @car.user_name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:user_phone) %>:</strong></dt>
  <dd><%= @car.user_phone %></dd>
  <dt><strong><%= model_class.human_attribute_name(:username_id) %>:</strong></dt>
  <dd><%= @car.username_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:phone) %>:</strong></dt>
  <dd><%= @car.phone %></dd>
  <dt><strong><%= model_class.human_attribute_name(:telefon) %>:</strong></dt>
  <dd><%= @car.telefon %></dd>
  <dt><strong><%= model_class.human_attribute_name(:telefonba) %>:</strong></dt>
  <dd><%= @car.telefonba %></dd>
  <dt><strong><%= model_class.human_attribute_name(:datum) %>:</strong></dt>
  <dd><%= @car.datum %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              cars_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_car_path(@car), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              car_path(@car),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
